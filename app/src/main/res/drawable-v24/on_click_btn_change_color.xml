<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>

            <solid
                android:color="#b1b3b4" />
            <stroke
                android:width="1dp"
                android:color="#868383" />
            <corners
                android:radius="20dp" />

        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#ecf2f4"
                android:endColor="#ecf2f4"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#868383" />
            <corners
                android:radius="20dp" />

        </shape>
    </item>
</selector>